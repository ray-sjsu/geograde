generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  name      String?
  googleId  String   @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model StudySpot {
  id              String   @id @default(cuid())
  name            String
  noiseLevel      Int?
  environmentType String
  streetNumber    Int?
  streetName      String
  cityName        String
  zipCode         Int
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}
